<%= render 'menus', :lessons => @lessons %>

<div class="row">
  <div class="span8 top20">
    <h5><%= @lesson.category.name %> / <%= @lesson.name %></h5>
  </div>

  <% @practices.each do |p| %>
    <div class="span8">
      <h5>
        <a href="/practices/<%= p.id %>"><%= p.name %></a> 
        <%= render 'practices/points', :practice => p %>
      </h5>
    </div>
  <% end %>

</div>